{
    "version": 2,
    "builds": [
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        },
        {
            "src": "api/py/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/py/(.*)",
            "dest": "/api/py/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}